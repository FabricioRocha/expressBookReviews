{
    "configurations": [
        {
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: debug",
            "request": "launch",
            "command": "npm run debug",
            "cwd": "${workspaceFolder}\\final_project"
        }
    ]
}